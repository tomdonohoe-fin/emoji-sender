{
    "compilerOptions": {
      "target": "es5",                          
      "module": "commonjs",                    
      "lib": [
        "es5",
        "es6"
     ],                   
      "outDir": "dist",                          
      "strict": true,         
      "noImplicitAny": true,
      "esModuleInterop": true,
      "resolveJsonModule": true,
      "emitDecoratorMetadata": true,
      "experimentalDecorators": true,
      "strictPropertyInitialization": false,
    },
    "exclude": ["node_modules", "dist"]
}